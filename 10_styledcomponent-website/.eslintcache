 #c4c3ca;\n  z-index: 100;\n`\n\nconst ImgWrapper = styled.div`\n  position: absolute;\n\ttop: 0;\n\tleft: 0;\n\twidth: 100%;\n\theight: 100%;\n\tz-index:2;\n`\n\nconst Img = styled.div`\n  position: absolute;\n\twidth: calc(100% - 80px);\n  max-width: 950px;\n\tbackground-size: cover;\n\tbackground-position:center center;\n\tbackground-repeat:no-repeat;\n\ttop: 50%;\n\tleft: 50%;\n\toverflow: hidden;\n\tlist-style:none;\n  border-radius: 20px;\n  box-shadow: 0 0 50px rgba(0,0,0,.6);\n\t-webkit-transform: translate(-50%, -50%);\n\ttransform: translate(-50%, -50%);\n\t-webkit-transition: all 300ms linear;\n\ttransition: all 300ms linear;\n  height: 500px;\n`\nconst calc = o => `translateY(${o * 1}px)`;\nexport const Hero = () => {\n  const ref = useRef();\n  const [{ offset }, set] = useSpring(() => ({ offset: 0 }));\n\n  const handleScroll = () => {\n    const posY = ref.current.getBoundingClientRect().top;\n    const offset = window.pageYOffset - posY;\n    set({ offset });\n  };\n\n  useEffect(() => {\n    window.addEventListener(\"scroll\", handleScroll);\n\n    return () => {\n      window.removeEventListener(\"scroll\", handleScroll);\n    };\n  });\n  return (\n    <div ref={ref}>\n      <SideTile>work</SideTile>\n      <HeroSection>\n        <Wrapper style={{ transform: offset.interpolate(calc) }}>\n          <Paragraph>We do magic</Paragraph>\n          <Heading>成熟社会<br />Consotium</Heading>\n        </Wrapper>\n      </HeroSection>\n      <ImgSection>\n        <ImgWrapper>\n          <Img><img src=\"http://www.ivang-design.com/svg-load/parallax/light.jpg\" alt=\"\" /></Img>\n        </ImgWrapper>\n      </ImgSection>\n    </div>\n  );\n};\nexport default Hero;\n\n/*\n\n      <div class=\"section full-height z-bigger\">\n  <ul class=\"case-study-wrapper\">\n    <li class=\"case-study-name\">\n      <a href=\"#\" class=\"hover-target\">light</a>\n    </li>\n    <li class=\"case-study-name\">\n      <a href=\"#\" class=\"hover-target\">flare</a>\n    </li>\n  